<Window x:Class="NonFlyweightGarage.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Non-Flyweight Garage" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10">
            <TextBox x:Name="TypeTextBox" Width="100" Margin="0,0,10,0" />
            <Button Content="Add Vehicle" Click="AddVehicle_Click"/>
        </StackPanel>
        <Canvas Name="GarageCanvas" Background="LightGray" Margin="10,48,10,10"/>
    </Grid>
</Window>
